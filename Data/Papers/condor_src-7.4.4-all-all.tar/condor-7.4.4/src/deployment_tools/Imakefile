PROGRAMS = 	uniq_pid_command

all_target($(PROGRAMS))

CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
LIB = $(STD_LIBS)

OBJ = uniq_pid_tool_main.o


/* C++ Tools */
public_c_plus_target(uniq_pid_command,sbin,$(OBJ),$(LIB))
public_link_target(sbin,uniq_pid_command,uniq_pid_midwife)
public_link_target(sbin,uniq_pid_command,uniq_pid_undertaker)


/* Perl Tools */
public_copy_target(cleanup_release,sbin,cleanup_release,EXECUTABLE_MODE)
public_copy_target(condor_local_stop,sbin,condor_local_stop,EXECUTABLE_MODE)
public_copy_target(condor_cleanup_local,sbin,condor_cleanup_local,EXECUTABLE_MODE)
public_copy_target(condor_cold_start,sbin,condor_cold_start,EXECUTABLE_MODE)
public_copy_target(condor_cold_stop,sbin,condor_cold_stop,EXECUTABLE_MODE)
public_copy_target(condor_config_bind,sbin,condor_config_bind,EXECUTABLE_MODE)
public_copy_target(filelock_midwife,sbin,filelock_midwife,EXECUTABLE_MODE)
public_copy_target(filelock_undertaker,sbin,filelock_undertaker,EXECUTABLE_MODE)
public_copy_target(condor_install_local,sbin,condor_install_local,EXECUTABLE_MODE)
public_copy_target(condor_local_start,sbin,condor_local_start,EXECUTABLE_MODE)
public_copy_target(install_release,sbin,install_release,EXECUTABLE_MODE)

/* Perl libaries */
public_copy_target(Execute.pm,lib,Execute.pm,OBJECT_MODE)
public_copy_target(FileLock.pm,lib,FileLock.pm,OBJECT_MODE)


