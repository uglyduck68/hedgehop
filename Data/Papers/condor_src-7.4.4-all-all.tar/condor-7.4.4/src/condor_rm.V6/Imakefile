NAME = condor_rm
all_target($(NAME))

CFLAGS = $(STD_C_FLAGS) $(CONFIGURE_PCRE_CFLAGS)
C_PLUS_FLAGS = -I../condor_schedd $(STD_C_PLUS_FLAGS) $(CONFIGURE_PCRE_CFLAGS)
LIB = $(STD_LIBS) $(CONFIGURE_PCRE_LDFLAGS)

OBJ = rm.o

public_c_plus_target($(NAME),bin,$(OBJ),$(LIB))

public_link_target(bin,condor_rm,condor_hold)
public_link_target(bin,condor_rm,condor_release)
public_link_target(bin,condor_rm,condor_vacate_job)

