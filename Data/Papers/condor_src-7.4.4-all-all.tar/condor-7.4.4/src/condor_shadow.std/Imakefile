NAME = condor_shadow.std
all_target($(NAME))

CFLAGS = $(STD_C_FLAGS) -DSYSCALL_DEBUG $(CONFIGURE_PCRE_CLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -DSYSCALL_DEBUG $(CONFIGURE_PCRE_CLAGS)
LIB = $(STD_LIBS) $(CONFIGURE_PCRE_LDFLAGS)

RC_OBJ = extern_path.o getmnt.o

OBJ = $(RC_OBJ) ops.o pseudo_ops.o shadow.o shadow_common.o \
	receivers.o log_events.o job_report.o

public_c_plus_target($(NAME),sbin,$(OBJ),$(LIB))

html:

IMPORT_LINKS = ../../config/import_links
import_objs(../condor_syscall_lib,receivers.o)
import_objs(../condor_syscall_lib,extern_path.o)
import_objs(../condor_util_lib,getmnt.o)

DEPEND_SRC = job_report.cpp log_events.cpp ops.cpp pseudo_ops.cpp shadow.cpp \
shadow_common.cpp

depend_source_target($(DEPEND_SRC))

testbin:: all
