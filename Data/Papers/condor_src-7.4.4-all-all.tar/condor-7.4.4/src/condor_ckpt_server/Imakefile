NAME = condor_ckpt_server
all_target($(NAME) ckpt_server_api.a)

CFLAGS = $(STD_C_FLAGS) $(CONFIGURE_PCRE_CFLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) $(CONFIGURE_PCRE_CFLAGS)
LIB = $(STD_LIBS) $(CONFIGURE_PCRE_LDFLAGS)

OBJ = network2.o gen_lib.o fileinfo2.o fileindex2.o imds2.o xferstat2.o \
	signal2.o alarm2.o server2.o xfer_summary.o replication.o protocol.o

SERVER_API_OBJ = network2.o server_interface.o

library_target(ckpt_server_api.a,$(SERVER_API_OBJ))

public_c_plus_target($(NAME),sbin,$(OBJ),$(LIB))

