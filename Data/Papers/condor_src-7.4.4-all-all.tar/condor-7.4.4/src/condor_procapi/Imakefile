all_target(libprocapi.a condor_procapi)

C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) $(CONFIGURE_PCRE_CFLAGS)
CFLAGS = $(STD_C_FLAGS) $(CONFIGURE_PCRE_CFLAGS)
LIB = $(STD_LIBS) $(CONFIGURE_PCRE_LDFLAGS)

OBJ = procapi.o  processid.o procapi_killfamily.o

TEST_OBJS = procapi_t.o procapi_getproc_t.o \
	procapi_fork_t.o procapi_perm_t.o \
	procapi_cpu_t.o 

library_target(libprocapi.a,$(OBJ))

c_plus_target(condor_procapi,$(TEST_OBJS),$(LIB))


c_plus_target(testprocapi,testprocapi.o,$(LIB))
html_target( procapi.h procinterface.h )

testbin_target(condor_procapi,755)

release:: all
stripped:: all
static:: all
testbin:: all
