NAME = condor_submit
all_target($(NAME))

#if IS_CLIPPED
CLIPPED = -DCLIPPED
#endif

CFLAGS = $(STD_C_FLAGS) $(CLIPPED) $(CONFIGURE_PCRE_CFLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) $(CLIPPED) $(CONFIGURE_PCRE_CFLAGS)
LIB = $(STD_LIBS) $(CONFIGURE_PCRE_LDFLAGS)

OBJ = submit.o

public_c_plus_target($(NAME),bin,$(OBJ),$(LIB))

html:
testbin:: all
